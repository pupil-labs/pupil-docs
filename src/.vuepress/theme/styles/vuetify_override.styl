.warning
  .warning--text
    color #e7c000 !important

.pl-logo
  margin: auto

.v-toolbar__extension
  background-color: #263238

.v-toolbar__content > *:last-child
  margin auto

.v-btn
  text-transform unset
  &.page-nav-btn
    font-size 14px

.v-card
  border-radius 6px

.sidebar
  .v-list__tile
    height 28px

  .v-list__tile__title
    font-weight bold
    font-size 14px

.gap-container .v-toolbar__content
  gap 8px

.howto-container
  .v-expansion-panel
    box-shadow unset

@media only screen and (min-width: 1264px)
  .container
    max-width 1000px

@media only screen and (min-width: 1904px)
  .container
  .submenu_container
  .toolbar_container
    max-width 1000px

@import '~vuetify/src/stylus/app.styl'
@import './media-query.styl'

.right
  float: unset !important

ul, ol
  padding 0 0 24px 20px

code
  box-shadow: unset
  font-weight 400

  &:before, &:after
    content ''

p, li
 code
  display inline